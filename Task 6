using Microsoft.VisualBasic;
using System;
using System.Collections;
using System.Xml;

public class Program
{
    static void Main()
    {
        string num = Console.ReadLine();
        int sum = 0;

        foreach (char p in num)
        {
            sum += Convert.ToInt32(p.ToString());
        }
        Console.WriteLine(sum);
        Console.ReadKey();
    }
}
