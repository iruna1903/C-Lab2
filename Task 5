using Microsoft.VisualBasic;
using System;
using System.Collections;
using System.Xml;

public class Program
{
    static void Main()
    {
        int year;
        year = Convert.ToInt32(Console.ReadLine());
        if (year % 4 == 0)
            if (year % 100 != 0)
                Console.WriteLine("Рік високосний");
            else
                Console.WriteLine("Ршк не високосний");
        Console.ReadKey();
    }
}
