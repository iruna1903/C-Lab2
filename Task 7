using Microsoft.VisualBasic;
using System;
using System.Collections;
using System.Xml;
public class Program
{
    static void Main()
     {
        string num = Console.ReadLine();
        int count = 0;
        foreach (char p in num)
        {
            if (p % 2 != 0)
                count++;
            else
                break;
        }
        if (count == num.Length)
            Console.WriteLine("Всі числа є непарними");
        else
            Console.WriteLine("Не всі числа є парними");
        Console.ReadKey();
    }
}
